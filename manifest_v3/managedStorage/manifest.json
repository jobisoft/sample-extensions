{
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "action": {
        "default_title": "Managed Storage Example"
    },
    "description": "An example options ui using values from the managed storage",
    "manifest_version": 3,
    "name": "Managed Storage Example",
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "permissions": [
        "storage"
    ],
    "version": "1.1",
    "browser_specific_settings": {
        "gecko": {
            "id": "managed-storage@examples.thunderbird.net"
        }
    }
}
